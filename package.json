{
  "name": "react-bootstrap-submit",
  "version": "1.2.2",
  "description": "Enforces valid react bootstrap form inputs before submit.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/landerton/react-bootstrap-submit.git"
  },
  "author": "Landon Anderton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/landerton/react-bootstrap-submit/issues"
  },
  "homepage": "https://github.com/landerton/react-bootstrap-submit",
  "keywords": [
    "react-component",
    "react-bootstrap",
    "react-bootstrap-validation",
    "react"
  ],
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
