    "react-bootstrap": "^0.31.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-bootstrap": "^0.31.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10"
  },
  "dependencies": {
    "validator": "^7.0.0"
  },
  "main": "index.js"
